            <version>${saxon.version}</version>
        </dependency>
        <dependency>
            <groupId>net.sf.saxon</groupId>
            <artifactId>saxon-xpath</artifactId>
            <version>${saxon.version}</version>
        </dependency>
        <dependency>
            <groupId>commons-dbcp</groupId>
