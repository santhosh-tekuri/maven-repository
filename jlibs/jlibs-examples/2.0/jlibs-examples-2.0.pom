    <name>examples</name>

    <properties>
        <saxon.version>9.0.0.2</saxon.version>
    </properties>

    <dependencies>
        <dependency>
            <groupId>jlibs</groupId>
            <artifactId>jlibs-i18n</artifactId>
            <version>${project.version}</version>
        </dependency>
        <dependency>
            <groupId>jlibs</groupId>
            <artifactId>jlibs-i18n-apt</artifactId>
            <version>${project.version}</version>
            <optional>true</optional>
        </dependency>
        <dependency>
            <groupId>jlibs</groupId>
            <artifactId>jlibs-swing</artifactId>
            <version>${project.version}</version>
        </dependency>
        <dependency>
            <groupId>jlibs</groupId>
            <artifactId>jlibs-jdbc</artifactId>
            <version>${project.version}</version>
        </dependency>
        <dependency>
            <groupId>jlibs</groupId>
            <artifactId>jlibs-jdbc-apt</artifactId>
            <version>${project.version}</version>
